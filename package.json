{
  "name": "app-treino",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome check",
    "format": "biome format --write",
    "seed": "ts-node prisma/seed.ts",
    "db:setup": "pnpm db:migrate && pnpm db:generate",
    "db:migrate": "dotenv -e .env -- prisma migrate dev --schema prisma/schema.prisma",
    "db:migrate-deploy": "dotenv -e .env -- prisma migrate deploy --schema prisma/schema.prisma",
    "db:migrate-resolve": "dotenv -e .env -- prisma migrate resolve --applied 20251108123916_add_schemas --schema prisma/schema.prisma",
    "db:push": "dotenv -e .env -- prisma db push --schema prisma/schema.prisma",
    "db:generate": "dotenv -e .env -- prisma generate --schema prisma/schema.prisma",
    "db:reset": "dotenv -e .env -- prisma migrate reset --schema prisma/schema.prisma",
    "db:create-only": "dotenv -e .env -- prisma migrate dev --create-only --schema prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "6.19.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tanstack/react-query": "^5.90.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.556.0",
    "next": "16.0.8",
    "react": "19.2.1",
    "react-day-picker": "^9.11.1",
    "react-dom": "19.2.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "ts-node": "^10.9.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "dotenv-cli": "^11.0.0",
    "prisma": "^6.19.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
